{
  "name": "genrx",
  "version": "0.0.1",
  "license": "Apache 2.0",
  "author": {
    "name": "Jonas Janz",
    "email": "janz.jonas@gmail.com"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:PixelJonas/genrx.git"
  },
  "homepage": "https://github.com/PixelJonas/genrx",
  "bugs": "https://github.com/PixelJonas/genrx/issues",
  "description": "a collection of generic implementations for @ngrx/store and @ngrx/effects",
  "main": "dist/genrx.umd.js",
  "module": "dist/genrx.esm.js",
  "jsnext:main": "dist/genrx.esm.js",
  "scripts": {
    "copy": "node tools/copy-package.js && node tools/cleanup.js",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc",
    "postbundle": "node tools/remove-code.js",
    "build": "npm run bundle && npm run copy",
    "push": "git push --tags origin HEAD:master",
    "release": "npm version minor && npm run build",
    "snapshot": "npm version patch && npm run build"
  },
  "types": "dist/index.d.ts",
  "private": false,
  "keywords": [
    "angular",
    "ngrx"
  ],
  "peerDependencies": {
    "@angular/core": "^4.1.3",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.3",
    "@ngrx/router-store": "^1.2.6",
    "@ngrx/store": "^2.2.2",
    "core-js": "2.4.1",
    "rxjs": "^5.1.0",
    "typescript": "^2.2.1",
    "zone.js": "^0.8.5",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@angular/compiler": "^4.1.3",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/platform-server": "^4.1.3",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.60",
    "del": "^3.0.0",
    "node-sass": "^4.5.3",
    "rimraf": "^2.5.4",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1"
  }
}
